<div class="container">
    <table class="table table-striped table-responsive">
        <thead>
        <tr>
            <th scope="col">User ID</th>
            <th scope="col">Post ID</th>
            <th scope="col">Title</th>
        </tr>
        </thead>
        <tbody>
        {{#each posts}}
            <tr>
                <td>{{userId}}</td>
                <td>{{id}}</td>
                <td>{{title}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>